<!DOCTYPE html>
<html>
<head>
    <title>Books</title>
</head>
<body>
<h1>Books</h1>
<a href="/books/upload">Upload New Book</a>
<table>
    <thead>
    <tr>
        <th>ID</th>
        <th>Title</th>
        <th>Author</th>
        <th>File URL</th>
    </tr>
    </thead>
    <tbody>
    {{#books}}
        <tr>
            <td>{{id}}</td>
            <td>{{title}}</td>
            <td>{{author}}</td>
            <td><a href="{{fileUrl}}">{{fileUrl}}</a></td>
        </tr>
    {{/books}}
    </tbody>
</table>
</body>
</html>
